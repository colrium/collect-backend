	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "attach",
			"name": "Auth app",
			"port": 9229,
			"restart": true,
			"stopOnEntry": false,
			"preLaunchTask": "start:debug:auth",
			"postDebugTask": "stop:bg:all",
			"protocol": "inspector",
			"skipFiles": [
				"<node_internals>/**/*.js",
				"${workspaceRoot}/node_modules/**/*.js"
			]
		}
	]
